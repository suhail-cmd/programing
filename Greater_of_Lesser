#python
class Solution:
    # @param A : list of integers
    # @param B : list of integers
    # @param C : integer
    # @return an integer
    def solve(self, A, B, C):
        count1,count2 = 0,0
        for num in A:
            if num > C:
                count1 += 1
        for num in B:
            if num < C:
                count2 += 1
        return max(count1,count2)
