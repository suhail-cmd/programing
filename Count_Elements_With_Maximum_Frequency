#python
class Solution:
    def maxFrequencyElements(self, nums: List[int]) -> int:
        freq = {}
        for num in nums:
            if num not in freq:
                freq[num] =  1
            else:
                freq[num] += 1
        max_freq = max(freq.values())
        total = 0
        for key,value in freq.items():
            if value == max_freq:
                total += value
        return total
        
