#python
class Solution:
	# @param A : tuple of integers
	# @param B : integer
	# @return an integer
	def findCount(self, A, B):
        def firstOccurence(A,B):
            low,high = 0,len(A)-1
            first = -1
            while low <= high:
                mid = (low + high)//2
                if A[mid] == B:
                    first = mid
                    high = mid - 1
                elif B > A[mid]:
                    low = mid + 1
                else:
                    high = mid - 1
            return first
        def lastOccurence(A,B):
            low,high = 0,len(A)-1
            last = -1
            while low <= high:
                mid = (low + high) // 2
                if A[mid] == B:
                    last = mid
                    low = mid + 1
                elif B < A[mid]:
                    high = mid  -  1
                else:
                    low = mid + 1
            return last
        first = firstOccurence(A,B)
        last = lastOccurence(A,B)
        if first == -1:
            return 0
        return last - first + 1
            
            
