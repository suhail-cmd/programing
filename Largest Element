#python

class Solution:
    def largestElement(self, nums):
        max_ = nums[0]
        for num in nums:
            if num > max_:
                max_ = num
        return max_
        
#java

class Solution {
    public int largestElement(int[] nums) {
        int max = nums[0];
        for(int i = 1;i < nums.length;i++){
            if(nums[i] > max){
                max = nums[i];
            }
        }
    return(max);
    }
}
